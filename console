#!/usr/bin/env php
<?php
set_time_limit(0);

/* INIT */
require_once 'autoload.php'; #composer autoloader

$kernel = new Kernel();
$kernel['consoleMode'] = true;
$app = $kernel->getContainer();
$app['console'] = new \Asgard\Console\Application('Asgard Framework', $kernel::VERSION, $app);
$kernel->load();

$app['console']->run()->send();